 material Spaceinvader/Alien_0_0_U
 {
     technique
     {
         pass
         {
             lighting off
 
             ambient 1 1 1 1
             diffuse 1 1 1 1
             specular 0 0 0 0
             emissive 0 0 0 

             scene_blend alpha_blend
             depth_write off

             texture_unit
             {
                 texture Resources/game/Alien_1_1.png 2.5
				 colour_op_ex source1 src_manual src_current 0.4 0.4 0.4
			tex_coord_set 0
             }
         }
     }
 }
 
